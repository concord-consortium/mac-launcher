<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="2">
    <pkg-ref id="org.concord.CCLauncher"/>
    <options customize="never" require-scripts="false"/>
    <volume-check>
        <allowed-os-versions>
            <os-version min="10.9"/>
        </allowed-os-versions>
    </volume-check>
    <choices-outline>
        <line choice="default">
            <line choice="org.concord.CCLauncher"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="org.concord.CCLauncher" visible="false" title="CC Launcher">
        <pkg-ref id="org.concord.CCLauncher"/>
    </choice>
    <pkg-ref id="org.concord.CCLauncher" onConclusion="none">CCLauncher.pkg</pkg-ref>
</installer-gui-script>
